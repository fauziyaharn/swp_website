{
  "version": 2,
  "functions": {
    "api/process.py": {
      "runtime": "python3.11"
    }
  },
  "routes": [
    { "src": "/api/process", "dest": "api/process.py" },
    { "src": "/health", "dest": "api/health.py" },
    { "src": "/api/health", "dest": "api/health.py" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
